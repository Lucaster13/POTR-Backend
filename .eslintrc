{
    "env": {
        "browser": true,
        "es2021": true
    },

    "extends": ["airbnb", "airbnb-typescript", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "parser": "@typescript-eslint/parser",
    "rules": {
        "@typescript-eslint/no-explicit-any": "warn"
    }
}
